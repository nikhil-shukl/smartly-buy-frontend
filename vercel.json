{
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "techtrendydeals.com"
        }
      ],
      "destination": "https://www.techtrendydeals.com",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "techtrendydeals.com"
        }
      ],
      "destination": "https://www.techtrendydeals.com/:1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/((?!404).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/404.html",
      "headers": [
        {
          "key": "x-vercel-status",
          "value": "404"
        }
      ]
    }
  ]
}
