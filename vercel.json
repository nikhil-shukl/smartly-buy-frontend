{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/((?!api|_next|404|favicon\\.ico|robots\\.txt|sitemap\\.xml|.*\\..*).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/404.html",
      "headers": [
        {
          "key": "x-vercel-status",
          "value": "404"
        }
      ]
    }
  ]
}
